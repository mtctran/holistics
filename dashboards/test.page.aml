Dashboard test {
  title: 'Test'
  description: 'Testing dashboard'
  block title: TextBlock {
    content: @md # My Dashboard;;
  }
  block v1: VizBlock {
    label: 'Customer Transaction'
    viz: DataTable {
      dataset: client_analytics
      theme {
        table {
          general {
            bg_color: '#E9EFFB'
          }
        }
      }
      fields: [
        VizFieldFull {
          ref: ref('customer_transactions', 'client_id')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('customer_transactions', 'customer_id')
          format {
            type: 'number'
            pattern: 'inherited'
          }
        },
        VizFieldFull {
          ref: ref('customer_transactions', 'name')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('customer_transactions', 'surname')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('customer_transactions', 'category')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('customer_transactions', 'transaction_amount')
          format {
            type: 'number'
            pattern: 'inherited'
          }
          uname: 'customer_transactions_transaction_amount'
        }
      ]
      settings {
        show_row_number: true
        conditional_formats: [
          ConditionalFormat {
            key: 'customer_transactions_transaction_amount'
            format: SingleFormat {
              condition {
                operator: 'greater_than'
                value: 500
              }
              text_color: '#328159'
              background_color: '#B5E3CD'
            }
          }
        ]
        row_limit: 5000
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }
  block v2: VizBlock {
    label: 'Total Customer'
    viz: MetricKpi {
      dataset: client_analytics
      value: VizFieldFull {
        label: 'Total Customer'
        ref: ref('customer_transactions', 'customer_id')
        aggregation: 'count'
        format {
          type: 'number'
          pattern: '#,###'
        }
      }
      settings {
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
        alignment: 'center'
      }
    }
    settings {
      hide_label: true
    }
  }
  block v3: VizBlock {
    label: 'Total Revenue'
    viz: MetricKpi {
      dataset: client_analytics
      value: VizFieldFull {
        label: 'Total Revenue'
        ref: ref('customer_transactions', 'transaction_amount')
        aggregation: 'sum'
        format {
          type: 'number'
          pattern: '#,###'
        }
      }
      settings {
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
        alignment: 'center'
      }
    }
    settings {
      hide_label: true
    }
  }
  block v4: VizBlock {
    label: 'Avg Revenue'
    viz: MetricKpi {
      dataset: client_analytics
      calculation avg_revenue {
        label: 'Avg Revenue'
        formula: @aql sum(customer_transactions, customer_transactions.transaction_amount) / count_distinct(customer_transactions, customer_transactions.customer_id);;
        calc_type: 'measure'
        data_type: 'number'
      }
      value: VizFieldFull {
        label: 'Avg Revenue'
        ref: 'avg_revenue'
        format {
          type: 'number'
          pattern: 'inherited'
        }
      }
      settings {
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
        alignment: 'center'
      }
    }
    settings {
      hide_label: true
    }
  }
  block v5: VizBlock {
    label: 'Sum of Transaction Amount by Category'
    viz: BarChart {
      dataset: client_analytics
      x_axis: VizFieldFull {
        ref: ref('customer_transactions', 'category')
        format {
          type: 'text'
        }
      }
      legend: VizFieldFull {
        ref: ref('customer_transactions', 'category')
        format {
          type: 'text'
        }
      }
      y_axis {
        label: 'Revenue'
        settings {
          show_data_label_by: 'value'
          stack_series_by: 'value'
        }
        series {
          field: VizFieldFull {
            ref: ref('customer_transactions', 'transaction_amount')
            aggregation: 'sum'
            format {
              type: 'number'
              pattern: '#,###'
            }
          }
        }
      }
      settings {
        row_limit: 5000
        sort {
          field_index: 0
          direction: 'desc'
          type: 'yaxis'
        }
        legend_label: 'hidden'
        x_axis_show_null_datetime: false
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }

  view: TabLayout {
    label: 'View 1'
    tab tab1: CanvasLayout {
      label: 'Tab 1'
      height: 840
      grid_size: 20
      block title {
        position: pos(460, 20, 240, 60)
      }
      block v1 {
        position: pos(20, 260, 540, 380)
        layer: 1
      }
      block v2 {
        position: pos(40, 80, 240, 120)
        layer: 2
      }
      block v3 {
        position: pos(460, 80, 240, 120)
        layer: 3
      }
      block v4 {
        position: pos(920, 80, 240, 120)
        layer: 4
      }
      block v5 {
        position: pos(620, 260, 540, 380)
        layer: 5
      }
      mobile {
        mode: 'auto'
      }
    }
    tab tab2: CanvasLayout {
      label: 'Untitled'
      height: 800
      grid_size: 20
      mobile {
        mode: 'auto'
      }
    }
  }

  theme: H.themes.vanilla
}