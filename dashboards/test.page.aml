Dashboard test {
  title: 'Test'
  description: 'Testing dashboard'
  block title: TextBlock {
    content: @md # My Dashboard;;
  }
  block v1: VizBlock {
    label: 'Customer Transaction'
    viz: DataTable {
      dataset: client_analytics
      theme {
        table {
          general {
            bg_color: '#E9EFFB'
          }
        }
      }
      fields: [
        VizFieldFull {
          ref: ref('customer_transactions', 'client_id')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('customer_transactions', 'customer_id')
          format {
            type: 'number'
            pattern: 'inherited'
          }
        },
        VizFieldFull {
          ref: ref('customer_transactions', 'name')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('customer_transactions', 'surname')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('customer_transactions', 'category')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('customer_transactions', 'transaction_amount')
          format {
            type: 'number'
            pattern: 'inherited'
          }
          uname: 'customer_transactions_transaction_amount'
        }
      ]
      settings {
        show_row_number: true
        conditional_formats: [
          ConditionalFormat {
            key: 'customer_transactions_transaction_amount'
            format: SingleFormat {
              condition {
                operator: 'greater_than'
                value: 500
              }
              text_color: '#328159'
              background_color: '#B5E3CD'
            }
          }
        ]
        row_limit: 5000
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }
  block v2: VizBlock {
    label: 'Total Customer'
    viz: MetricKpi {
      dataset: client_analytics
      value: VizFieldFull {
        label: 'Total Customer'
        ref: ref('customer_transactions', 'customer_id')
        aggregation: 'count'
        format {
          type: 'number'
          pattern: '#,###'
        }
      }
      settings {
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
        alignment: 'center'
      }
    }
    settings {
      hide_label: true
    }
  }
  block v3: VizBlock {
    label: 'Total Revenue'
    viz: MetricKpi {
      dataset: client_analytics
      value: VizFieldFull {
        label: 'Total Revenue'
        ref: ref('customer_transactions', 'transaction_amount')
        aggregation: 'sum'
        format {
          type: 'number'
          pattern: '#,###'
        }
      }
      settings {
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
        alignment: 'center'
      }
    }
    settings {
      hide_label: true
    }
  }

  view: TabLayout {
    label: 'View 1'
    tab tab1: CanvasLayout {
      label: 'Tab 1'
      height: 840
      grid_size: 20
      block title {
        position: pos(460, 20, 240, 60)
      }
      block v1 {
        position: pos(20, 260, 540, 380)
        layer: 1
      }
      block v2 {
        position: pos(160, 80, 240, 120)
        layer: 2
      }
      block v3 {
        position: pos(740, 80, 240, 120)
        layer: 3
      }
      mobile {
        mode: 'auto'
      }
    }
    tab tab2: CanvasLayout {
      label: 'Untitled'
      height: 800
      grid_size: 20
      mobile {
        mode: 'auto'
      }
    }
  }

  theme: H.themes.vanilla
}