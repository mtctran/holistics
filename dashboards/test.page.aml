Dashboard test {
  title: 'Test'
  description: 'Testing dashboard'
  block title: TextBlock {
    content: @md # My Dashboard;;
  }
  block v1: VizBlock {
    label: 'Customer ID, First Name, Last Name, and Client ID'
    viz: DataTable {
      dataset: client_analytics
      fields: [
        VizFieldFull {
          ref: ref('customer_transactions', 'customer_id')
          format {
            type: 'number'
            pattern: 'inherited'
          }
        },
        VizFieldFull {
          ref: ref('customer_transactions', 'name')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('customer_transactions', 'surname')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('customer_transactions', 'client_id')
          format {
            type: 'text'
          }
        }
      ]
      settings {
        show_row_number: true
        row_limit: 5000
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }
  block v2: VizBlock {
    label: 'Customer ID, First Name, and Last Name'
    viz: DataTable {
      dataset: client_analytics
      fields: [
        VizFieldFull {
          ref: ref('customer_transactions', 'customer_id')
          format {
            type: 'number'
            pattern: 'inherited'
          }
        },
        VizFieldFull {
          ref: ref('customer_transactions', 'name')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('customer_transactions', 'surname')
          format {
            type: 'text'
          }
        }
      ]
      settings {
        show_row_number: true
        row_limit: 5000
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }

  view: TabLayout {
    label: 'View 1'
    tab tab1: CanvasLayout {
      label: 'Tab 1'
      height: 840
      grid_size: 20
      block title {
        position: pos(460, 20, 240, 60)
      }
      block v1 {
        position: pos(20, 20, 540, 380)
        layer: 1
      }
      block v2 {
        position: pos(720, 20, 400, 300)
        layer: 2
      }
      mobile {
        mode: 'auto'
      }
    }
    tab tab2: CanvasLayout {
      label: 'Untitled'
      height: 800
      grid_size: 20
      mobile {
        mode: 'auto'
      }
    }
  }

  theme: H.themes.vanilla
}