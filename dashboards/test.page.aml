Dashboard test {
  title: 'Test'
  description: 'Testing dashboard'
  block title: TextBlock {
    content: @md # My Dashboard;;
  }
  block v1: VizBlock {
    label: 'Revenue'
    viz: MetricKpi {
      dataset: ecommerce
      calculation revenue {
        label: 'Revenue'
        formula: @aql sum(order_items, products.price * order_items.quantity);;
        calc_type: 'measure'
        data_type: 'number'
      }
      value: VizFieldFull {
        ref: 'revenue'
        format {
          type: 'number'
          pattern: 'inherited'
        }
      }
      settings {
        display_mode: 'compare_by_number'
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
        label {
          font_size: 25
        }
        alignment: 'center'
      }
    }
    settings {
      hide_label: true
    }
  }
  block v2: VizBlock {
    label: 'Customer ID, First Name, and Last Name'
    viz: DataTable {
      dataset: client_analytics
      fields: [
        VizFieldFull {
          ref: ref('customer_transactions', 'customer_id')
          format {
            type: 'number'
            pattern: 'inherited'
          }
        },
        VizFieldFull {
          ref: ref('customer_transactions', 'name')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('customer_transactions', 'surname')
          format {
            type: 'text'
          }
        }
      ]
      settings {
        show_row_number: true
        row_limit: 5000
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }

  view: TabLayout {
    label: 'View 1'
    tab tab1: CanvasLayout {
      label: 'Tab 1'
      height: 840
      grid_size: 20
      block title {
        position: pos(460, 20, 240, 60)
      }
      block v1 {
        position: pos(20, 80, 200, 120)
        layer: 1
      }
      block v2 {
        position: pos(720, 20, 400, 300)
        layer: 2
      }
      mobile {
        mode: 'auto'
      }
    }
    tab tab2: CanvasLayout {
      label: 'Untitled'
      height: 800
      grid_size: 20
      mobile {
        mode: 'auto'
      }
    }
  }

  theme: H.themes.vanilla
}