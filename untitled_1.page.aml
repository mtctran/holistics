Dashboard untitled_1 {
  title: 'Untitled'
  block title: TextBlock {
    content: @md # Your title goes here ;;
  }
  block v1: VizBlock {
    label: 'Customer ID, Client ID, First Name, and Last Name'
    viz: DataTable {
      dataset: client_analytics
      fields: [
        VizFieldFull {
          ref: ref('customer_transactions', 'customer_id')
          format {
            type: 'number'
            pattern: 'inherited'
          }
        },
        VizFieldFull {
          ref: ref('customer_transactions', 'client_id')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('customer_transactions', 'name')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('customer_transactions', 'surname')
          format {
            type: 'text'
          }
        }
      ]
      settings {
        show_row_number: true
        row_limit: 5000
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }
  view: CanvasLayout {
    label: 'View 1'
    height: 840
    grid_size: 20
    block title {
      position: pos(20, 20, 1160, 60)
    }
    block v1 {
      position: pos(20, 100, 400, 300)
      layer: 1
    }
    mobile {
      mode: 'auto'
    }
  }
  theme: H.themes.vanilla
}